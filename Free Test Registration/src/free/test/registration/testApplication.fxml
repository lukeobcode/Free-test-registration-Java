<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="348.0" prefWidth="511.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="free.test.registration.FXMLDocumentController">
    <children>
        <Button fx:id="testApplicationButton" layoutX="206.0" layoutY="283.0" onAction="#applyForTest" text="Apply for a test" />
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <Text layoutX="134.0" layoutY="62.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Test Application">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Text>
      <CheckBox fx:id="symptomsCB" layoutX="132.0" layoutY="212.0" mnemonicParsing="false" text="I  am experiencing COVID-19 Symptoms" />
      <CheckBox fx:id="smsCB" layoutX="132.0" layoutY="235.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="221.0" text="I would like to be contacted via SMS" />
