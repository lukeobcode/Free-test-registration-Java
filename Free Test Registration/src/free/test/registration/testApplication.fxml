      <TextField fx:id="emailConfirmTF" layoutX="160.0" layoutY="91.0" prefHeight="27.0" prefWidth="197.0" promptText="Confirm Email" />
      <TextField fx:id="deliverHouseTF" layoutX="160.0" layoutY="151.0" prefHeight="27.0" prefWidth="198.0" promptText="House number to be delivered to " />
      <TextField fx:id="phoneConfirmTF" layoutX="161.0" layoutY="121.0" prefHeight="27.0" prefWidth="198.0" promptText="Confirm Phone Number" />
      <TextField fx:id="postcodeDeliveryTF" layoutX="160.0" layoutY="180.0" prefHeight="27.0" prefWidth="198.0" promptText="Postcode to be delivered to " />
